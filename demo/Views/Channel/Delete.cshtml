@{
    ViewBag.Title = "Delete Channel";
}

<h2>Delete Channel</h2>
<h1>Are you sure you want to delete this channel?</h1>
<h3>@Model.ChannelName</h3>
<form method="post">
    <input type="hidden" name="Id" value="@Model.ChannelId" />
    <input type="submit" value="Yes" class="btn btn-danger" name="dcsn" />
    <input type="submit" value="No" class="btn btn-warning" name="dcsn" />
</form>

